{{#each punkAPI}}
<img class="randomBeerImg" src="{{this.image_url}}" alt="random-beer-img">
<p>{{this.name}}</p>
<p>{{this.description}}</p>
<p>{{this.tagline}}</p>
<h3>Superb food pairings!</h3>
<ul>
{{#each this.food_pairing}}
<li>{{this}}</li>
{{/each}}
</ul>
<h3>Some tips from the own brewers:</h3>
<p>{{this.brewers_tips}}</p>
{{/each}}